{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "linebreak-style": 0,
    "indent": [1, "tab"],
    "no-tabs": "off",
    "arrow-parens": "off",
    "object-curly-spacing": "off",
    "require-jsdoc": "off",
    "comma-dangle": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
